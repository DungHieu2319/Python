n=int(input())
for i in range(1, n+1):
    print(i, end=' ')
else: print()
for i in range(n, -1, -1):
    print(i, end=' ')
else: print()
for i in range(0, n+1, 2):
    if i%2==0:
        print(i, end=' ')
else: print()
for i in range(1, n+1, 2):
    print(i,end=' ')
else: print()   
for i in range(0, n, 4):
    print(i, end=' ')
else: print(end='\n')
for i in range(n):
    print(chr(ord('a')+i), end=' ')
else: print()
for i in range(n):
    print(chr(ord('a')+26-(n-i)), end=' ')
